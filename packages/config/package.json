{
  "name": "@rhino-project/config",
  "version": "0.21.0-beta.26",
  "description": "",
  "author": "JP Rosevear",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhino-project/rhino-project.git",
    "directory": "packages/config"
  },
  "homepage": "https://rhino-project.org",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./assets": {
      "import": {
        "types": "./dist/esm/assets.d.ts",
        "default": "./dist/esm/assets.js"
      },
      "require": {
        "types": "./dist/cjs/assets.d.cts",
        "default": "./dist/cjs/assets.cjs"
      }
    },
    "./env": {
      "import": {
        "types": "./dist/esm/env.d.ts",
        "default": "./dist/esm/env.js"
      },
      "require": {
        "types": "./dist/cjs/env.d.cts",
        "default": "./dist/cjs/env.cjs"
      }
    },
    "./rhino-env": {
      "import": {
        "types": "./dist/esm/rhino-env.d.ts"
      },
      "require": {
        "types": "./dist/cjs/rhino-env.d.cts"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./coverage",
    "test:types": "tsc",
    "test:lib": "vitest",
    "test:lib:dev": "pnpm run test:lib --watch",
    "test:build": "publint --strict",
    "test:eslint": "eslint --ext .ts,.tsx --max-warnings 0 ./src ",
    "build": "vite build"
  },
  "keywords": [],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-copy": "^3.5.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  }
}
